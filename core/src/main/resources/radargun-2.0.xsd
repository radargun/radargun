   <!--From radargun-core-2.0.0-SNAPSHOT.jar/org.radargun.stages.cache.CheckCacheDataStage-->
   <complexType name="check-cache-data">
      <annotation>
         <documentation>Stage for checking presence or absence of data entered in other stages.</documentation>
      </annotation>
      <complexContent>
         <extension base="rg:abstract-dist">
