<schema xmlns:rg="urn:radargun:benchmark:2.0" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="urn:radargun:benchmark:2.0" version="1.0" xmlns="http://www.w3.org/2001/XMLSchema">
   <simpleType name="int">
      <union>
         <simpleType>
            <restriction base="integer"/>
         </simpleType>
         <simpleType>
            <restriction base="string">
               <pattern value="[$#]\{.*\}"/>
            </restriction>
         </simpleType>
      </union>
   </simpleType>
   <element name="benchmark">
      <complexType>
         <sequence>
            <element maxOccurs="1" minOccurs="0" name="master">
               <complexType>
                  <attribute name="bindAddress" type="string"/>
                  <attribute name="port" type="rg:int"/>
               </complexType>
            </element>
            <choice maxOccurs="1" minOccurs="0">
               <element maxOccurs="1" minOccurs="0" name="local">
                  <complexType/>
               </element>
               <element maxOccurs="1" minOccurs="0" name="clusters">
                  <complexType>
                     <choice maxOccurs="unbounded" minOccurs="1">
                        <element maxOccurs="unbounded" minOccurs="0" name="scale">
                           <complexType>
                              <sequence>
                                 <element name="cluster" type="rg:cluster_base"/>
                              </sequence>
                              <attribute name="from" type="rg:int" use="required"/>
                              <attribute name="to" type="rg:int" use="required"/>
                              <attribute name="inc" type="rg:int"/>
                           </complexType>
                        </element>
                        <element name="cluster" type="rg:cluster"/>
                     </choice>
                  </complexType>
               </element>
            </choice>
            <element maxOccurs="1" minOccurs="1" name="configurations">
               <complexType>
                  <sequence>
                     <element maxOccurs="unbounded" minOccurs="1" name="config">
                        <complexType>
                           <sequence>
                              <element maxOccurs="unbounded" minOccurs="1" name="setup">
                                 <complexType>
                                    <sequence>
                                       <element maxOccurs="unbounded" minOccurs="0" name="property" type="rg:property"/>
                                    </sequence>
                                    <attribute name="plugin" type="string" use="required"/>
                                    <attribute name="file" type="string"/>
                                    <attribute name="service" type="string"/>
                                    <attribute name="group" type="string"/>
                                 </complexType>
                              </element>
                           </sequence>
                           <attribute name="name" type="string" use="required"/>
                        </complexType>
                     </element>
                  </sequence>
               </complexType>
            </element>
            <element maxOccurs="1" minOccurs="0" name="init" type="rg:scenario-init"/>
            <element maxOccurs="1" minOccurs="1" name="scenario">
               <complexType>
                  <sequence>
                     <choice maxOccurs="unbounded" minOccurs="1">
                        <element name="repeat" type="rg:repeat"/>
                        <element name="background-statistics-start" type="rg:background-statistics-start"/>
                        <element name="background-statistics-stop" type="rg:background-statistics-stop"/>
                        <element name="background-stressors-check" type="rg:background-stressors-check"/>
                        <element name="background-stressors-start" type="rg:background-stressors-start"/>
                        <element name="background-stressors-stop" type="rg:background-stressors-stop"/>
                        <element name="check-cache-data" type="rg:check-cache-data"/>
                        <element name="check-topology" type="rg:check-topology"/>
                        <element name="clear-cache" type="rg:clear-cache"/>
                        <element name="stress-test" type="rg:stress-test"/>
                        <element name="web-session-benchmark" type="rg:stress-test"/>
                        <element name="client-stress-test" type="rg:client-stress-test"/>
                        <element name="cluster-validation" type="rg:cluster-validation"/>
                        <element name="data-for-query" type="rg:data-for-query"/>
                        <element name="distributed-task" type="rg:distributed-task"/>
                        <element name="isolation-level-check" type="rg:isolation-level-check"/>
                        <element name="jmx-cluster-validation-prepare" type="rg:jmx-cluster-validation-prepare"/>
                        <element name="jmx-cluster-validation" type="rg:jmx-cluster-validation"/>
                        <element name="j-profiler" type="rg:j-profiler"/>
                        <element name="jvm-monitor-start" type="rg:jvm-monitor-start"/>
                        <element name="jvm-monitor-stop" type="rg:jvm-monitor-stop"/>
                        <element name="load-file" type="rg:load-file"/>
                        <element name="map-reduce" type="rg:map-reduce"/>
                        <element name="parallel-start-stop" type="rg:parallel-start-stop"/>
                        <element name="query" type="rg:query"/>
                        <element name="random-data" type="rg:random-data"/>
                        <element name="service-start" type="rg:service-start"/>
                        <element name="service-stop" type="rg:service-stop"/>
                        <element name="set-log-level" type="rg:set-log-level"/>
                        <element name="set-partitions" type="rg:set-partitions"/>
                        <element name="single-tx-check" type="rg:single-tx-check"/>
                        <element name="single-tx-load" type="rg:single-tx-load"/>
                        <element name="sleep" type="rg:sleep"/>
                        <element name="stack-trace-watchdog" type="rg:stack-trace-watchdog"/>
                        <element name="stress-test-warmup" type="rg:stress-test-warmup"/>
                        <element name="web-session-warmup" type="rg:stress-test-warmup"/>
                        <element name="tpcc-benchmark" type="rg:tpcc-benchmark"/>
                        <element name="tpcc-population" type="rg:tpcc-population"/>
                        <element name="wait-for-topology-event" type="rg:wait-for-topology-event"/>
                        <element name="wait-for-topology-settle" type="rg:wait-for-topology-settle"/>
                        <element name="write-skew-check" type="rg:write-skew-check"/>
                        <element name="xs-repl-check" type="rg:xs-repl-check"/>
                        <element name="xs-repl-load" type="rg:xs-repl-load"/>
                     </choice>
                  </sequence>
               </complexType>
            </element>
            <element maxOccurs="1" minOccurs="0" name="cleanup" type="rg:scenario-cleanup"/>
            <element maxOccurs="1" minOccurs="1" name="reports">
               <complexType>
                  <sequence>
                     <element maxOccurs="unbounded" minOccurs="1" name="reporter">
                        <complexType>
                           <sequence>
                              <element maxOccurs="unbounded" minOccurs="0" name="report">
                                 <complexType>
                                    <element name="property" type="rg:property"/>
                                 </complexType>
                              </element>
                              <element maxOccurs="1" minOccurs="0" name="properties">
                                 <complexType>
                                    <sequence>
                                       <element name="property" type="rg:property"/>
                                    </sequence>
                                 </complexType>
                              </element>
                           </sequence>
                           <attribute name="type" type="string" use="required"/>
                           <attribute name="run" type="rg:org-radargun-config-reporterconfiguration-runcondition"/>
                        </complexType>
                     </element>
                  </sequence>
               </complexType>
            </element>
         </sequence>
      </complexType>
   </element>
   <complexType name="cluster_base">
      <sequence>
         <element maxOccurs="unbounded" minOccurs="0" name="group">
            <complexType>
               <attribute name="name" type="string" use="required"/>
               <attribute name="size" type="rg:int" use="required"/>
            </complexType>
         </element>
      </sequence>
   </complexType>
   <complexType name="cluster">
      <complexContent>
         <extension base="rg:cluster_base">
            <attribute name="size" type="rg:int"/>
         </extension>
      </complexContent>
   </complexType>
   <complexType name="property">
      <complexContent>
         <simpleContent/>
         <attribute name="name" type="string" use="required"/>
      </complexContent>
   </complexType>
